{
    "name": "qview3d",
    "author": "SUNY Hydra Lab",
    "description": "QView3D Vue Client",
    "productName": "QView3D",
    "license": "MIT",
    "version": "0.9.1",
    "private": true,
    "type": "module",

    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "test:unit": "vitest",
        "build-only": "vite build",
        "type-check": "vue-tsc --build --force",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "start-flask-with-reload": "cd .. && .venv\\Scripts\\activate && cd ./server && flask run --debug --extra-files ../server/",
        "start-flask": "cd .. && .venv\\Scripts\\activate && cd ./server && flask run",
        "run-prod": "npm run build-only && start-flask",
        "start-vue": "npm run dev",
        "start": "npm-run-all --parallel start-flask start-vue"
    },

    "dependencies": {
        "@cyhnkckali/vue3-color-picker": "^2.0.0",
        "@fortawesome/fontawesome-free": "^6.5.1",
        "@vuepic/vue-datepicker": "^8.5.0",
        "@vueuse/components": "^11.1.0",
        "@vueuse/core": "^11.1.0",
        "ansi-to-html": "^0.7.2",
        "axios": "^1.6.5",
        "bootstrap": "^5.3.3",
        "bootstrap-daterangepicker": "^3.1.0",
        "cors": "^2.8.5",
        "file-saver": "^2.0.5",
        "gcode-preview": "^2.18.0",
        "node": "^21.6.2",
        "serve-handler": "^6.1.5",
        "socket.io-client": "^4.7.4",
        "three": "^0.171.0",
        "toast": "^0.5.4",
        "vue": "^3.3.11",
        "vue-router": "^4.2.5",
        "vue-socket.io-extended": "^4.2.0",
        "vue-toast-notification": "^3.1.2",
        "vue3-toast": "^0.0.1",
        "vuedraggable": "^4.1.0"
    },

    "devDependencies": {
        "@rushstack/eslint-patch": "^1.3.3",
        "@tsconfig/node18": "^18.x.x",
        "@types/bootstrap": "^5.2.10",
        "@types/file-saver": "^2.0.7",
        "@types/jsdom": "^21.1.6",
        "@types/node": "^18.19.3",
        "@types/three": "^0.171.0",
        "@vitejs/plugin-vue": "^4.5.2",
        "@vitejs/plugin-vue-jsx": "^3.1.0",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.3",
        "@vue/tsconfig": "^0.5.0",
        "eslint": "^8.49.0",
        "eslint-plugin-vue": "^9.17.0",
        "jsdom": "^23.0.1",
        "prettier": "^3.0.3",
        "rollup": "^4.24.0",
        "typescript": "~5.3.0",
        "vite": "^5.2.9",
        "vitest": "^1.0.4",
        "vue-tsc": "^1.8.25",
        "npm-run-all": "^4.1.5"
    }
}
